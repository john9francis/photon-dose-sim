# our vis driver
/vis/open OGL
# initialize geometry
/vis/drawVolume
# set view angle
/vis/viewer/set/viewpointVector 1 1 1
/vis/viewer/set/lightsVector 1 0 0
# set style
/vis/viewer/set/style wireframe
/vis/viewer/set/auxiliaryEdge true
# make world invisible
/vis/geometry/set/visibility World 0 false